<% provide(:title, "Edit Group Name") %>
<h1>Edit Group Name</h1>

<div class="row">
  <div class="col-lg-6 col-lg-offset-3">
    <%= form_for(@prac_group, url: {action: "update"}) do |f| %>
      <%= render 'shared/error_messages', object: f.object %>

      <table class="table">
        <tr>
          <td><%= f.label "Practice Group Name:" %></td>
          <td><%= f.text_field :group_name %></td>
        </tr>
      </table>
      <div class="center">
       <%= f.submit "Update Group Name", class: "btn btn-large btn-primary" %>
     </div>
    <% end %>
  </div>
</div>
