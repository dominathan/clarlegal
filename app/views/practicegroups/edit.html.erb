<% provide(:title, "Edit Group Name") %>
<h2>Edit Group Name</h2>

<div class="row">
  <div class="col-lg-6 col-lg-offset-3">
    <%= form_for(@prac_group, url: {action: "update"}, html: { class: "form-horizontal" }) do |f| %>
      <%= render 'shared/error_messages', object: f.object %>

       <div class="form-group">
          <div class="col-sm-3">
            <%= f.label :group_name, "Practice Group Name:", class: "control-label" %>
          </div>
          <div class="col-sm-9">
            <%= f.text_field :group_name, class: "form-control" %>
          </div>
        </div>
      <div class="center">
       <%= f.submit "Update Group Name", class: "btn btn-large btn-primary" %>
     </div>
    <% end %>
  </div>
</div>
