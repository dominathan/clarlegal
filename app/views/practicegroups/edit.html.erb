<% provide(:title, "Edit Group Name") %>
<h1>Edit Group Name</h1>

<div class="row">
  <div class="span6 offset3">
    <%= form_for(@prac_group, url: {action: "update"}) do |f| %>
      <%= render 'shared/error_messages', object: f.object %>

      <%= f.label "Practice Group Name:" %>
      <%= f.text_field :group_name %>

       <%= f.submit "Update Group Name", class: "btn btn-large btn-primary" %>

    <% end %>
  </div>
</div>
