<% provide(:title, "New Practice Group") %>

<h1>Add Practice Group to <%= "#{@lawfirm.firm_name}" %> </h1>

<div class="row">
  <div class="span6 offset3">
    <%= form_for(@prac_group, url: {action: "create"}) do |f| %>
      <%= render 'shared/error_messages', object: f.object %>

      <%= f.label "Practice Group Name:" %>
      <%= f.text_field :group_name %>

       <%= f.submit "Add Practice Group", class: "btn btn-large btn-primary" %>

    <% end %>
  </div>
</div>
